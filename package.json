{
  "name": "50ra4-react-component",
  "version": "0.1.1",
  "private": true,
  "main": "dist/es6/index.js",
  "module": "dist/es6/index.js",
  "typings": "dist/es6/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "npm run build",
    "start": "npm run storybook",
    "test": "react-scripts test",
    "test-ci": "cross-env CI=true react-scripts test",
    "clean": "rimraf ./dist",
    "build-check": "tsc --noEmit",
    "build-es6": "tsc -p ./tsconfig.build-es6.json",
    "build": "npm run clean && npm run build-es6",
    "storybook": "start-storybook -p 6006 -s public --no-dll",
    "build-storybook": "rimraf ./docs && build-storybook -s public -o docs --no-dll"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run build-check && lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "50ra4-library": "git+https://github.com/shigarashi1/50ra4-library.git",
    "@date-io/date-fns": "1.3.13",
    "@material-ui/core": "4.11.0",
    "@material-ui/icons": "4.9.1",
    "@material-ui/pickers": "3.2.10",
    "@material-ui/styles": "4.10.0",
    "@reduxjs/toolkit": "1.4.0",
    "date-fns": "2.16.1",
    "material-table": "1.69.2",
    "notistack": "1.0.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-redux": "7.2.2",
    "styled-components": "5.2.1"
  },
  "devDependencies": {
    "@babel/core": "7.12.3",
    "@storybook/addon-actions": "6.0.28",
    "@storybook/addon-essentials": "6.0.28",
    "@storybook/addon-links": "6.0.28",
    "@storybook/node-logger": "6.0.28",
    "@storybook/preset-create-react-app": "3.1.4",
    "@storybook/react": "6.0.28",
    "@testing-library/jest-dom": "5.11.5",
    "@testing-library/react": "11.1.0",
    "@testing-library/user-event": "12.1.10",
    "@types/jest": "26.0.15",
    "@types/node": "12.19.3",
    "@types/react": "16.9.55",
    "@types/react-dom": "16.9.9",
    "@types/react-redux": "7.1.11",
    "@types/styled-components": "5.1.4",
    "@typescript-eslint/eslint-plugin": "4.6.1",
    "@typescript-eslint/parser": "4.6.1",
    "babel-loader": "8.1.0",
    "cross-env": "7.0.2",
    "eslint": "7.12.1",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-prettier": "3.1.4",
    "husky": "4.3.0",
    "lint-staged": "10.5.1",
    "prettier": "2.1.2",
    "react-is": "17.0.1",
    "react-scripts": "4.0.0",
    "rimraf": "3.0.2",
    "stylelint": "13.7.2",
    "ts-jest": "26.4.3",
    "ts-node": "9.0.0",
    "typescript": "4.0.5",
    "web-vitals": "0.2.4"
  }
}
